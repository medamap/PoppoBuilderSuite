  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.10.0",
    "better-sqlite3": "^11.10.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "colors": "^1.4.0",
    "commander": "^11.1.0",
    "express": "^5.1.0",
    "i18next": "^23.7.6",
    "i18next-fs-backend": "^2.3.1",
    "js-yaml": "^4.1.0",
    "prom-client": "^15.1.0",
