    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "mocha": "^11.6.0",
    "sinon": "^21.0.0",
    "sinon-chai": "^4.0.0",
    "socket.io-client": "^4.8.1"
