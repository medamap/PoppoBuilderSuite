{
  "name": "poppo-builder-suite",
  "version": "0.1.0",
  "description": "AI-powered autonomous software development system",
  "main": "src/minimal-poppo.js",
  "bin": {
    "poppo": "./scripts/poppo-process.js",
    "poppobuilder": "./bin/poppobuilder.js"
  },
  "scripts": {
    "start": "node src/minimal-poppo.js",
    "start:agents": "node scripts/start-agents.js",
    "restart-scheduler": "node scripts/restart-scheduler.js",
    "setup": "node scripts/setup-labels.js",
    "dashboard": "open http://localhost:3001",
    "trace": "node scripts/trace.js",
    "audit": "node scripts/audit.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:notification": "jest test/notification*.test.js --coverage",
    "test:providers": "jest test/*-provider.test.js --coverage",
    "test:integration": "jest test/notification-integration.test.js",
    "test:mocha": "mocha test/**/*.test.js --timeout 10000",
    "config": "node scripts/poppo-config.js",
    "config:show": "node scripts/poppo-config.js show",
    "config:hierarchy": "node scripts/poppo-config.js hierarchy",
    "config:validate": "node scripts/poppo-config.js validate",
    "config:env": "node scripts/poppo-config.js env",
    "poppo": "node scripts/poppo-process.js",
    "poppo:status": "node scripts/poppo-process.js status",
    "poppo:help": "node scripts/poppo-process.js help",
    "analytics": "node scripts/poppo-analytics.js",
    "analytics:report": "node scripts/poppo-analytics.js report",
    "analytics:stats": "node scripts/poppo-analytics.js stats",
    "analytics:archive": "node scripts/poppo-analytics.js archive",
    "demo:logs": "node examples/log-management-demo.js",
    "demo:scheduler": "node examples/task-scheduler-demo.js"
  },
  "keywords": [
    "ai",
    "automation",
    "github",
    "claude"
  ],
  "author": "medamap",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.10.0",
    "better-sqlite3": "^11.10.0",
    "chalk": "^4.1.2",
    "colors": "^1.4.0",
    "commander": "^11.1.0",
    "express": "^5.1.0",
    "i18next": "^23.7.6",
    "i18next-fs-backend": "^2.3.1",
    "js-yaml": "^4.1.0",
    "uuid": "^9.0.1",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "chai": "^5.2.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "mocha": "^11.6.0",
    "sinon": "^21.0.0",
    "sinon-chai": "^4.0.0"
  }
}
