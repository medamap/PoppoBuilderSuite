    "benchmark:agents": "node scripts/benchmark-cli.js quick --type agents",
    "benchmark:system": "node scripts/benchmark-cli.js quick --type system",
    "benchmark:reports": "node scripts/benchmark-cli.js reports",
    "benchmark:full": "node scripts/benchmark-cli.js run --full"
