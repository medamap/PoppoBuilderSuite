    "express": "^5.1.0",
    "i18next": "^23.16.8",
    "i18next-fs-backend": "^2.6.0",
    "js-yaml": "^4.1.0",
    "node-cron": "^3.0.3",
    "prom-client": "^15.1.0",
    "redis": "^5.5.6",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "chai": "^5.2.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "mocha": "^11.6.0",
