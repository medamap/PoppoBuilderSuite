# E2Eテスト用環境変数
NODE_ENV=test

# テスト用GitHub設定（モックを使用）
GITHUB_TOKEN=test-token-12345
GITHUB_WEBHOOK_SECRET=test-webhook-secret

# テスト用Claude設定（モックを使用）
CLAUDE_API_KEY=test-claude-api-key
CLAUDE_SESSION_DIR=./test/e2e/temp/claude-sessions

# テスト用データベース
DB_PATH=./test/e2e/temp/test.db

# テスト用ログ設定
LOG_DIR=./test/e2e/temp/logs
LOG_LEVEL=debug

# テスト用ポート
DASHBOARD_PORT=4001
WEBHOOK_PORT=4002

# セキュリティ設定（テスト用）
JWT_SECRET=test-jwt-secret-12345
SESSION_SECRET=test-session-secret-12345
DASHBOARD_PASSWORD=test-password

# エージェント設定
AGENT_COORDINATOR_ENABLED=true
AGENT_CCLA_ENABLED=true
AGENT_CCAG_ENABLED=true
AGENT_CCPM_ENABLED=true
AGENT_CCQA_ENABLED=true

# タイムアウト設定（短めに）
CLAUDE_TIMEOUT_MS=5000
PROCESS_TIMEOUT_MS=10000
TASK_TIMEOUT_MS=30000

# その他の設定
MAX_CONCURRENT_PROCESSES=2
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW_MS=60000